	, male_perc DECIMAL
	, american_indian_perc DECIMAL
	, asian_perc DECIMAL
	, black_perc DECIMAL
	, hispanic_perc DECIMAL
	, two_races_perc DECIMAL
	, white_perc DECIMAL
	, econ_disadvantage_perc DECIMAL
	, limited_english_perc DECIMAL
	, student_with_disability_perc DECIMAL
	, acad_or_int_gifted_perc DECIMAL
	, all_students INT
	, female INT
	, male INT
	, american_indian INT
	, asian INT
	, black INT
	, hispanic INT
	, two_races INT
	, white INT
	, econ_disatvantage INT
